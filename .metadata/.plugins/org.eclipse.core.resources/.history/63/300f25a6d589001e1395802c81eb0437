<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.avo.www.repository.MemberDAO">
	<update id="updateLastLogin">
		update member set mem_last_login = now() where mem_email = #{memEmail}
	</update>
	
	<!-- 해당 이메일이 있는지 확인 -->
	<select id="selectEmail" resultType="">
		select * from member where where mem_email = #{memEmail}
	</select>
	
	<select id="selectAuths" resultType="">
		select * from auth_member where email = #{email}
	</select>
</mapper>