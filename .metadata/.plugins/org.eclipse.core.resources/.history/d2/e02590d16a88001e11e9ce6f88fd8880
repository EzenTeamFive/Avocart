package com.avo.www.service;

import java.util.List;

import com.avo.www.domain.CommunityBoardVO;

public interface CommunityBoardService {

	int register(CommunityBoardVO bvo);

	List<CommunityBoardVO> getList();

	CommunityBoardVO getDetail(long cmBno);

	int modify(CommunityBoardVO bvo);

	int remove(long cmBno);

	int likeInsert(long cmBno, String cmEmail);

	int likeDelete(long cmBno, String cmEmail);

}
